{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      {
        "js": "ignorePackages"
      }
    ]
  }
}